{
  "name": "redis-gcra",
  "version": "0.4.0",
  "description": "Rate limiting based on Generic Cell Rate Algorithm",
  "main": "lib/index.js",
  "homepage": "https://github.com/Losant/redis-gcra#readme",
  "author": "Losant <hello@losant.com>",
  "license": "MIT",
  "keywords": [
    "GCRA",
    "redis",
    "limit",
    "throttle"
  ],
  "bugs": {
    "url": "https://github.com/Losant/redis-gcra/issues"
  },
  "directories": {
    "test": "./test",
    "example": "./examples",
    "lib": "./lib"
  },
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "lint": "esw . --ext .js",
    "lint:fix": "yarn lint --fix",
    "lint:watch": "yarn lint --watch",
    "lint:changed": "lint-staged",
    "test": "mocha test 2>&1",
    "test:watch": "mocha --watch test 2>&1",
    "reinstall": "rm -rf node_modules && yarn install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Losant/redis-gcra.git"
  },
  "lint-staged": {
    "*.js": "esw"
  },
  "dependencies": {
    "ioredis": ">=4.0.0 <6.0.0"
  },
  "devDependencies": {
    "@losant/eslint-config-losant": "^1.4.4",
    "husky": "^4.3.0",
    "lint-staged": "^13.0.3",
    "mocha": "^9.2.2",
    "should": "^13.2.3"
  },
  "eslintConfig": {
    "extends": "@losant/eslint-config-losant/env/node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "mocha": {
    "reporter": "spec",
    "recursive": true,
    "require": "should",
    "check-leaks": true
  }
}
